{
    "name": "Use YouTube to improve your pronunciation",
    "version": "1.0",
    "manifest_version": 2,
    "description": "YouGlish gives you fast, unbiased answers about how English is spoken by real people and in context.",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["youlish.js"]
    },
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "author": [ {
        "name": "Quy Nguyen",
        "email": "nguyenledinhquy@gmail.com"
    }]
}