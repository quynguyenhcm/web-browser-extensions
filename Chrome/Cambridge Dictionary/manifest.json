{
  	"name": "Lookup Cambridge Dictionary",
  	"version": "1.0",
  	"manifest_version": 2,
  	"description": "This extension looks up Cambridge Advanced Learner's Dictionary for the selected text",
  	"icons": { 
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
  	"browser_action": {
    	"default_icon": "icon.png"
  	},
  	"background": {
    	"scripts": ["cambridge.js"]
	},
	"permissions": [
        "activeTab",
        "contextMenus"
    ],
	"author": [ {
  		"name": "Quy Nguyen",
  		"email": "nguyenledinhquy@gmail.com"
 	}]
}