{
    "name": "Oxford Advanced Learner's Dictionary",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Look up online Oxford Advanced Learner's Dictionary for the selected text",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["oxford.js"]
    },
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "author": [ {
        "name": "Quy Nguyen",
        "email": "nguyenledinhquy@gmail.com"
    }]
}